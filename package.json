{
  "name": "mcp-lodestar",
  "version": "2.0.0",
  "description": "MCP Server for LodeStar title and closing cost calculations",
  "type": "module",
  "module": "src/index.ts",
  "main": "dist/index.js",
  "bin": {
    "lodestar-mcp": "dist/index.js"
  },
  "scripts": {
    "dev": "npx @smithery/cli dev",
    "build": "npm run build:http",
    "build:stdio": "tsc",
    "build:http": "npx @smithery/cli build",
    "start": "npm run start:http",
    "start:http": "node .smithery/index.cjs",
    "start:stdio": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest",
    "demo": "gtimeout 30s npm run start:stdio",
    "prepublishOnly": "npm run build:stdio"
  },
  "keywords": [
    "mcp",
    "lodestar",
    "title",
    "closing-costs",
    "real-estate"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.3",
    "axios": "^1.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@smithery/cli": "^1.2.17",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "moduleNameMapping": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
}
